#!/usr/bin/env python3

from sys import argv
from math import sqrt, isqrt


def read_test_file(file_name):
    """Function to read numbers from file: one on each line"""

    with open(file_name, "r", encoding="utf-8") as file:
        lines = file.readlines()

    number_to_factor = []

    for line in lines:
        number_to_factor.append(int(line.strip()))

    return number_to_factor
def is_perfect_square(n):
    """
    Returns True if n is a perfect square, False otherwise.
    """
    # If the square of the integer square root of n is equal to n, then n is a perfect square
    return isqrt(n) ** 2 == n

def find_numbers(n):
    """Function to find factors of current number"""

    last_digit = n % 10
    if last_digit == 0:
        start_range = 2
    elif last_digit % 2 == 0:
        start_range = 2
    elif last_digit == 5:
        start_range = 5
    elif last_digit == 9:
        if n % 3 == 0:
            start_range = 3
        elif is_perfect_square(n):
            start_range = isqrt(n)
        else:
            start_range = 1
    else:
        start_range = last_digit
    for i in range(start_range, int(sqrt(n)) + 1):
        if n % i == 0:
            print("{}={}*{}".format(n, n // i, i))
            break
    else:
        print("{}={}*1".format(n, n))

if len(argv) == 2:
    number = read_test_file(argv[1])[0]
    find_numbers(number)
