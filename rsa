#!/usr/bin/env python3

from sys import argv
from math import sqrt

def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(sqrt(n))+1):
        if n % i == 0:
            return False
    return True

def find_primes(n):
    for i in range(2, int(sqrt(n))+1):
        if n % i == 0:
            if is_prime(i) and is_prime(n//i):
                p, q = i, n//i
                print("{}={}*{}".format(n, p, q))
                return

if len(argv) == 2:
    file_name = argv[1]
    with open(file_name, "r") as file:
        n = int(file.readline())
    find_primes(n)

